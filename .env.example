# Shared auth token - must be the same on server, agent, and PWA
AUTH_TOKEN=change-me-to-a-strong-secret

# VAPID keys for Web Push notifications
# Generate with: npx web-push generate-vapid-keys
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=

# Relay server URL (for the agent to connect to)
# Use ws:// for local dev, wss:// for production
RELAY_URL=ws://localhost:3001

# Server port
PORT=3001

# Agent local HTTP port
HTTP_PORT=9680
